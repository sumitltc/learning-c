#include <stdio.h>

int main()
{
    int m1, m2, m3, m4, m5, per;
    
    printf("ENTER THE MARKS IN FIVE SUBJECTS");
    scanf("%d %d %d %d %d", &m1, &m2 ,&m3, &m4, &m5);
    
    per = (m1+m2+m3+m4+m5)* 100/500;
    
    if(per>=60)
    {
        printf("First Division\n");
    }
    else
    { 
        if(per>=50);
            printf("SECOND DIVISION\n");
        else
        {
            if(per>=40)
                printf("THIRD DIVISION\n");
            else
                printf("fail\n");
            
        }
    }

    return 0;
}
