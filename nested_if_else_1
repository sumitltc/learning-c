#include <stdio.h>

int main()
{
    int i;
    
    printf("ENTER THE NUMBER\n");
    scanf("%d",&i);
    
    if(i==1)
    {
        printf("YOU WOULD GO IN HEAVEN\n");
    }
    else
    {
        if(i==2)
        {
            printf("YOU WOULD GO TO HELL\n");
        }
        else
        {
            printf("THERE IS MOTHER EARTH\n");
        }
    }

    return 0;
}
